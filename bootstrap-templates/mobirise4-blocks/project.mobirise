          "_name": "menu2",
          "_customHTML": "<section class=\"menu\" group=\"Menu\" plugins=\"DropDown, TouchSwipe\" always-top global not-draggable position-absolute>\n    \n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"checkbox\" title=\"Show Logo\" name=\"showLogo\">\n        <input type=\"checkbox\" title=\"Show Brand Name\" name=\"showBrand\" checked>\n        <input type=\"checkbox\" title=\"Show Menu Items\" name=\"showItems\" checked>\n        <input type=\"checkbox\" title=\"Show Button(s)\" name=\"showButtons\" checked>\n        <input type=\"checkbox\" title=\"Sticky\" name=\"sticky\" checked>\n        <input type=\"checkbox\" title=\"Collapsed\" name=\"collapsed\">\n        <input type=\"checkbox\" title=\"Transparent\" name=\"transparent\" checked>\n        <input type=\"color\" title=\"Hamburger Color\" name=\"hamburgerColor\" value=\"#333333\">\n        <input type=\"color\" title=\"Background Color\" name=\"menuBgColor\" value=\"#232323\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <nav class=\"navbar navbar-dropdown align-items-center\" mbr-class=\"{\n        'navbar-fixed-top': sticky,\n        'navbar-toggleable-sm': !collapsed,\n        'collapsed': collapsed,\n        'bg-color transparent': transparent\n        }\">\n        <button class=\"navbar-toggler navbar-toggler-right\" type=\"button\" data-toggle=\"collapse\" data-target=\"#navbarSupportedContent\" aria-controls=\"navbarSupportedContent\" aria-expanded=\"false\" aria-label=\"Toggle navigation\">\n            <div class=\"hamburger\">\n                <span></span>\n                <span></span>\n                <span></span>\n                <span></span>\n            </div>\n        </button>\n        <div class=\"menu-logo\">\n            <div class=\"navbar-brand\">\n                <span mbr-if=\"showLogo\" class=\"navbar-logo\">\n                    <a href=\"https://mobirise.com\">\n                        <img src=\"../_images/logo2.png\" alt=\"Mobirise\">\n                    </a>\n                </span>\n                <span mbr-if=\"showBrand\" mbr-buttons class=\"navbar-caption-wrap\" data-toolbar=\"-mbrBtnMove,-mbrBtnAdd,-mbrBtnRemove\">\n                    <a class=\"navbar-caption display-4 text-white\" data-app-selector=\".navbar-caption\" href=\"https://mobirise.com\" data-app-btn=\"true\" data-toolbar=\"-mbrBtnMove,-mbrBtnAdd,-mbrBtnRemove\">\n                        MOBIRISE</a>\n                </span>\n            </div>\n        </div>\n        <div class=\"collapse navbar-collapse\" id=\"navbarSupportedContent\">\n            <ul mbr-if=\"showItems\" mbr-menu class=\"navbar-nav nav-dropdown display-4\" mbr-class=\"{'nav-right': !showButtons,\n                'navbar-nav-top-padding': isPublish && !showBrand && !showLogo}\"><li class=\"nav-item\"><a class=\"nav-link link text-white\" href=\"index.html\" data-app-selector=\".nav-link,.dropdown-item\" aria-expanded=\"true\">MOBIRISE 4 THEME</a></li><li class=\"nav-item dropdown open\">\n                    <a class=\"nav-link link dropdown-toggle text-white\" href=\"https://mobirise.com\" data-app-selector=\".nav-link,.dropdown-item\" data-toggle=\"dropdown-submenu\" aria-expanded=\"true\">\n                        BLOCKS</a><div class=\"dropdown-menu\"><a class=\"dropdown-item text-white\" href=\"intro-mobirise-4.html\" data-app-selector=\".nav-link, .dropdown-item\">Intros</a><a class=\"dropdown-item text-white\" href=\"images-videos-mobirise-4.html\" data-app-selector=\".nav-link, .dropdown-item\">Images &amp; Videos</a><a class=\"dropdown-item text-white\" href=\"sliders-mobirise-4.html\" data-app-selector=\".nav-link, .dropdown-item\">Slider Blocks</a><a class=\"dropdown-item text-white\" href=\"gallery-mobirise-4.html\" data-app-selector=\".nav-link, .dropdown-item\">Galleries</a><a class=\"dropdown-item text-white\" href=\"article-mobirise-4.html\" data-app-selector=\".nav-link, .dropdown-item\">Article Blocks</a><a class=\"dropdown-item text-white\" href=\"features-mobirise-4.html\" data-app-selector=\".nav-link, .dropdown-item\">Features Blocks</a><a class=\"dropdown-item text-white\" href=\"pricing-tables-mobirise-4.html\" data-app-selector=\".nav-link, .dropdown-item\">Tables &amp; Pricing Tables</a><a class=\"dropdown-item text-white\" href=\"counters-mobirise-4.html\" data-app-selector=\".nav-link, .dropdown-item\">Counters</a><a class=\"dropdown-item text-white\" href=\"countdowns-mobirise-4.html\" data-app-selector=\".nav-link, .dropdown-item\">Countdowns</a><a class=\"dropdown-item text-white\" href=\"progress-bars-mobirise-4.html\" data-app-selector=\".nav-link, .dropdown-item\">Progress Blocks</a><a class=\"dropdown-item text-white\" href=\"timelines-mobirise-4.html\" data-app-selector=\".nav-link, .dropdown-item\">Timelines</a><a class=\"dropdown-item text-white\" href=\"toggles-tabs-mobirise-4.html\" data-app-selector=\".nav-link, .dropdown-item\">Accordions &amp; Toggles &amp; Tabs</a><a class=\"dropdown-item text-white\" href=\"info-mobirise-4.html\" data-app-selector=\".nav-link, .dropdown-item\">Info Blocks</a><a class=\"dropdown-item text-white\" href=\"forms-mobirise-4.html\" data-app-selector=\".nav-link, .dropdown-item\">Forms</a><a class=\"dropdown-item text-white\" href=\"maps-mobirise-4.html\" data-app-selector=\".nav-link, .dropdown-item\">Maps</a><a class=\"dropdown-item text-white\" href=\"social-blocks-mobirise-4.html\" data-app-selector=\".nav-link, .dropdown-item\">Social Blocks</a><a class=\"dropdown-item text-white\" href=\"testimonials-mobirise-4.html\" data-app-selector=\".nav-link, .dropdown-item\">Testimonials</a><a class=\"dropdown-item text-white\" href=\"footers-mobirise-4.html\" data-app-selector=\".nav-link, .dropdown-item\">Footers</a></div>\n                </li></ul>\n            <div mbr-if=\"showButtons\" mbr-buttons class=\"navbar-buttons mbr-section-btn\"><a class=\"btn btn-sm btn-primary\" href=\"https://mobirise.com/m